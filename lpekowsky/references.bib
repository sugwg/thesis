	Date-Modified = {2011-07-01 15:28:41 -0400},
	Journal = {In proceedings of "A life with stars" (astro-ph/0608280)},
	Note = {astro-ph/0608280},
	Title = {{Effect of PSR J0737-3039 on the DNS Merger Rate and Implications for GW Detection}},
	Year = 2006}

