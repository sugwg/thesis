@inproceedings{Kim:2006,
	Abstract = { We present the current estimates of the Galactic merger rate of double-neutron-star (DNS) systems. Using a statistical analysis method, we calculate the probability distribution function (PDF) of the rate estimates, which allows us to assign confidence intervals to the rate estimates. We calculate the Galactic DNS merger rate based on the three known systems B1913+16, B1534+12, and J0737-3039. The discovery of J0737-3039 increases the estimated DNS merger rate by a factor ~6 than what is previously known. The most likely values of DNS merger rate lie in the range 3-190 per Myr depending on different pulsar models. Motivated by a strong correlation between the peak rate estimates and a pulsar luminosity function, we calculate a 'global' probability distribution as a single representation of the parameter space covered by different pulsar population models. We compare the global PDF with the observed supernova Ib/c rate, which sets an upper limit on the DNS merger rate. Finally, we remark on implications of new discoveries such as of J1756-2251, the 4th DNS in the Galactic disk, and J1906+0746, a possible DNS system.},
	Author = {{Kim}, C. and {Kalogera}, V. and {Lorimer}, D.~R.},
	Bibcode = {2006.astro-ph..0608280},
	Booktitle = {A life with stars},
	Comments = {9 pages, 2 figures, To appear in proceedings of "A life with stars", a conference in honour of Ed van den Heuvel (Amsterdam, August 2005), New Ast. Rev},
