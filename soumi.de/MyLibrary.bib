	month = jul,
	year = {1998},
	keywords = {HYDRODYNAMICS, BLACK HOLE PHYSICS, RELATIVITY, ACCRETION, RADIATION MECHANISMS: THERMAL, ACCRETION DISCS},
	pages = {739--746},
	file = {Full Text PDF:/Users/dsiegel/Zotero/storage/2EQ8I5ND/Beloborodov - 1998 - Super-Eddington accretion discs around Kerr black .pdf:application/pdf}
}

