@article{yang_can_2018,
	title = {Can {We} {Distinguish} {Low}-mass {Black} {Holes} in {Neutron} {Star} {Binaries}?},
	volume = {856},
	issn = {1538-4357},
	url = {http://stacks.iop.org/0004-637X/856/i=2/a=110?key=crossref.51e361d4eabc61849f8d01d6a9626753},
	doi = {10.3847/1538-4357/aab2b0},
	abstract = {The detection of gravitational waves (GWs) from coalescing binary neutron stars (NS) represents another milestone in gravitational-wave astronomy. However, since LIGO is currently not as sensitive to the merger/ringdown part of the waveform, the possibility that such signals are produced by a black hole (BH){\textendash}NS binary can not be easily ruled out without appealing to assumptions about the underlying compact object populations. We review a few astrophysical channels that might produce BHs below 3 Me (roughly the upper bound on the maximum mass of an NS), as well as existing constraints for these channels. We show that, due to the uncertainty in the NS equation of state, it is difficult to distinguish GWs from a binary NS system from those of a BH{\textendash}NS system with the same component masses, assuming Advanced LIGO sensitivity. This degeneracy can be broken by accumulating statistics from many events to better constrain the equation of state, or by third-generation detectors with higher sensitivity to the late-spiral to post-merger signal. We also discuss the possible differences in electromagnetic (EM) counterparts between binary NS and low-mass BH{\textendash}NS mergers, arguing that it will be challenging to definitively distinguish the two without better understanding of the underlying astrophysical processes.},
	language = {en},
	number = {2},
	urldate = {2019-04-18},
	journal = {The Astrophysical Journal},
	author = {Yang, Huan and East, William E. and Lehner, Luis},
	month = mar,
	year = {2018},
	pages = {110},
	file = {Yang et al. - 2018 - Can We Distinguish Low-mass Black Holes in Neutron.pdf:/Users/dsiegel/Zotero/storage/NQLGZANJ/Yang et al. - 2018 - Can We Distinguish Low-mass Black Holes in Neutron.pdf:application/pdf}
}

