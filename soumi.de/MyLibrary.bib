	pages = {40},
	file = {Submitted Version:/Users/dsiegel/Zotero/storage/7FV8B5SB/Ji et al. - 2019 - The Lanthanide Fraction Distribution in Metal-poor.pdf:application/pdf}
}

@article{melso_simulating_2019,
	title = {Simulating {Gas} {Inflow} at the {Disk}-{Halo} {Interface}},
