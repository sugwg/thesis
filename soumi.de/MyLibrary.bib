@article{khokhlov_deflagration--detonation_1997,
	title = {Deflagration-to-{Detonation} {Transition} in {Thermonuclear} {Supernovae}},
	volume = {478},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/1997ApJ...478..678K},
	doi = {10.1086/303815},
	abstract = {We derive the criteria for deflagration to detonation transition (DDT) in a Type Ia supernova. The theory is based on two major assumptions: (1) detonation is triggered via the Zeldovich gradient mechanism inside a region of mixed fuel and products, and (2) the mixed region is produced by a turbulent mixing of fuel and products either inside an active deflagration front or during the global expansion and subsequent contraction of an exploding white dwarf.

We determine the critical size of the mixed region required to initiate a detonation in a degenerate carbon-oxygen mixture. This critical length is much larger than the width of the reaction front of a Chapman-Jouguet detonation. However, at densities greater than {\textasciitilde}=5 {\texttimes}
106 g cm-3, it is much smaller than the size of a white dwarf. We derive the critical turbulent intensity required to create the mixed region inside an active deflagration front in which a detonation can form. We conclude that the density $\rho$tr at which a detonation can form in a carbon-oxygen white dwarf is low, less than 2-5 {\texttimes} 107 g cm-3 but greater than 5
{\texttimes} 106 g cm-3.},
	urldate = {2020-05-21},
	journal = {\apj},
	author = {Khokhlov, A. M. and Oran, E. S. and Wheeler, J. C.},
	month = mar,
	year = {1997},
	keywords = {Stars: Supernovae: General, Hydrodynamics, Stars: Interiors},
	pages = {678--688},
	file = {Full Text PDF:/Users/dsiegel/Zotero/storage/TBHIQT4I/Khokhlov et al. - 1997 - Deflagration-to-Detonation Transition in Thermonuc.pdf:application/pdf}
}

