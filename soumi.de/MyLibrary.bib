	doi = {10.1103/PhysRevLett.121.091102},
	abstract = {We use gravitational-wave observations of the binary neutron star merger GW170817 to explore the tidal deformabilities and radii of neutron stars. We perform a Bayesian parameter estimation with the source location and distance informed by electromagnetic observations. We also assume that the two stars have the same equation of state; we demonstrate that, for stars with masses comparable to the component masses of GW170817, this is effectively implemented by assuming that the stars' dimensionless tidal deformabilities are determined by the binary's mass ratio q by $\Lambda${\textless}SUB{\textgreater}1{\textless}/SUB{\textgreater}/$\Lambda${\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater}=q{\textless}SUP{\textgreater}6{\textless}/SUP{\textgreater}. We investigate different choices of prior on the component masses of the neutron stars. We find that the tidal deformability and 90\% credible interval is $\Lambda$ ? =222{\textless}SUB{\textgreater}-138{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+420{\textless}/SUP{\textgreater} for a uniform component mass prior, $\Lambda$ ? =245{\textless}SUB{\textgreater}-151{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+453{\textless}/SUP{\textgreater} for a component mass prior informed by radio observations of Galactic double neutron stars, and $\Lambda$ ? =233{\textless}SUB{\textgreater}-144{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+448{\textless}/SUP{\textgreater} for a component mass prior informed by radio pulsars. We find a robust measurement of the common areal radius of the neutron stars across all mass priors of 8.9 <=R {\textasciicircum}<=13.2 km , with a mean value of ?R {\textasciicircum}?=10.8 km . Our results are the first measurement of tidal deformability with a physical constraint on the star's equation of state and place the first lower bounds on the deformability and areal radii of neutron stars using gravitational waves.},
	language = {en},
	number = {9},
	urldate = {2019-07-09},
	journal = {Physical Review Letters},
	author = {De, Soumi and Finstad, Daniel and Lattimer, James M. and Brown, Duncan A. and Berger, Edo and Biwer, Christopher M.},
