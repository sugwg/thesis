@article{tchekhovskoy_magnetic_2015,
	title = {Magnetic flux of progenitor stars sets gamma-ray burst luminosity and variability},
	volume = {447},
	url = {https://ui.adsabs.harvard.edu/abs/2015MNRAS.447..327T/abstract},
	doi = {10.1093/mnras/stu2229},
	abstract = {Long-duration gamma-ray bursts (GRBs) are thought to come from the core collapse of Wolf-Rayet stars. Whereas their stellar masses M{\textless}SUB{\textgreater}*{\textless}/SUB{\textgreater} have a rather narrow distribution, the population of GRBs is very diverse, with gamma-ray luminosities L{\textless}SUB{\textgreater}$\gamma${\textless}/SUB{\textgreater} spanning several orders of magnitude. This suggests the existence of a `hidden' stellar variable whose burst-to-burst variation leads to a spread in L{\textless}SUB{\textgreater}$\gamma${\textless}/SUB{\textgreater}. Whatever this hidden variable is, its variation should not noticeably affect the shape of GRB light curves, which display a constant luminosity (in a time-average sense) followed by a sharp drop at the end of the burst seen with Swift/XRT. We argue that such a hidden variable is progenitor star's large-scale magnetic flux. Shortly after the core collapse, most of stellar magnetic flux accumulates near the black hole (BH) and remains there. The flux extracts BH rotational energy and powers jets of roughly a constant luminosity, L{\textless}SUB{\textgreater}j{\textless}/SUB{\textgreater}. However, once BH mass accretion rate dot\{M\} falls below ?L{\textless}SUB{\textgreater}j{\textless}/SUB{\textgreater}/c{\textless}SUP{\textgreater}2{\textless}/SUP{\textgreater}, the flux becomes dynamically important and diffuses outwards, with the jet luminosity set by the rapidly declining mass accretion rate, L\_j? dot\{M\}c{\textasciicircum}2. This provides a potential explanation for the sharp end of GRBs and the universal shape of their light curves. During the GRB, gas infall translates spatial variation of stellar magnetic flux into temporal variation of L{\textless}SUB{\textgreater}j{\textless}/SUB{\textgreater}. We make use of the deviations from constancy in L{\textless}SUB{\textgreater}j{\textless}/SUB{\textgreater} to perform stellar magnetic flux `tomography'. Using this method, we infer the presence of magnetized tori in the outer layers of progenitor stars for GRB 920513 and GRB 940210.},
	language = {en},
	number = {1},
	urldate = {2019-11-01},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Tchekhovskoy, Alexander and Giannios, Dimitrios},
	month = feb,
	year = {2015},
	pages = {327},
	file = {Full Text PDF:/Users/dsiegel/Zotero/storage/88S6FG7L/Tchekhovskoy and Giannios - 2015 - Magnetic flux of progenitor stars sets gamma-ray b.pdf:application/pdf;Snapshot:/Users/dsiegel/Zotero/storage/4AETPHHG/abstract.html:text/html}
}

@article{li_numerical_2019,
	title = {Numerical and perturbative computations of the fuzzy dark matter model},
	volume = {99},
	url = {https://ui.adsabs.harvard.edu/2019PhRvD..99f3509L/abstract},
	doi = {10.1103/PhysRevD.99.063509},
	abstract = {We investigate nonlinear structure formation in the fuzzy dark matter (FDM) model using both numerical and perturbative techniques. On the numerical side, we examine the virtues and limitations of a Schr{\"o}dinger-Poisson solver (wave formulation) versus a fluid dynamics solver (Madelung formulation). On the perturbative side, we carry out a computation of the one-loop mass power spectrum, i.e. up to third order in perturbation theory. We find that (1) in many situations, the fluid dynamics solver is capable of producing the expected interference patterns, but it fails in situations where destructive interference causes the density to vanish{\textemdash}a generic occurrence in the nonlinear regime. (2) The Schr{\"o}dinger-Poisson solver works well in all test cases, but it is demanding in resolution: suppose one is interested in the mass power spectrum on large scales, it is not sufficient to resolve structure on those same scales; one must resolve the relevant de Broglie scale which is often smaller. The fluid formulation does not suffer from this issue. (3) We compare the one-loop mass power spectrum from perturbation theory against the mass power spectrum from the Schr{\"o}dinger-Poisson solver, and find good agreement in the mildly nonlinear regime. We contrast fluid perturbation theory with wave perturbation theory; the latter has a more limited range of validity. (4) As an application, we compare the Lyman-alpha forest flux power spectrum obtained from the Schr{\"o}dinger-Poisson solver versus one from an N-body simulation (the latter is often used as an approximate method to make predictions for FDM). At redshift 5, the two, starting from the same initial condition, agree to better than 10\% on observationally relevant scales as long as the FDM mass exceeds 2 {\texttimes}10{\textless}SUP{\textgreater}-23{\textless}/SUP{\textgreater} eV . We emphasize that the so called quantum pressure is capable of both enhancing and suppressing fluctuations in the nonlinear regime{\textemdash}which dominates depends on the scale and quantity of interest.},
	language = {en},
	urldate = {2019-11-04},
	journal = {Physical Review D},
	author = {Li, Xinyu and Hui, Lam and Bryan, Greg L.},
	month = mar,
	year = {2019},
	pages = {063509},
	file = {Full Text PDF:/Users/dsiegel/Zotero/storage/QCU3GNYZ/Li et al. - 2019 - Numerical and perturbative computations of the fuz.pdf:application/pdf;Snapshot:/Users/dsiegel/Zotero/storage/P79BRUY7/abstract.html:text/html}
}

