	author = {Villar, V. A. and Cowperthwaite, P. S. and Berger, E. and Blanchard, P. K. and Gomez, S. and Alexander, K. D. and Margutti, R. and Chornock, R. and Eftekhari, T. and Fazio, G. G. and Guillochon, J. and Hora, J. L. and Nicholl, M. and Williams, P. K. G.},
