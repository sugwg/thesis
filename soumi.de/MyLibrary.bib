	keywords = {accretion, stars: black holes, accretion discs, stars: jets, (stars:) gamma-ray burst: general},
	pages = {4811--4825},
	file = {Submitted Version:/Users/dsiegel/Zotero/storage/JV3KTJU5/Christie et al. - 2019 - The role of magnetic field geometry in the evoluti.pdf:application/pdf}
