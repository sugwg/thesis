@article{bauswein_neutron-star_2017,
	title = {Neutron-star {Radius} {Constraints} from {GW170817} and {Future} {Detections}},
	volume = {850},
	url = {https://ui.adsabs.harvard.edu/abs/2017ApJ...850L..34B/abstract},
	doi = {10.3847/2041-8213/aa9994},
	abstract = {We introduce a new, powerful method to constrain properties of neutron stars (NSs). We show that the total mass of GW170817 provides a reliable constraint on the stellar radius if the merger did not result in a prompt collapse as suggested by the interpretation of associated electromagnetic emission. The radius \{R\}{\textless}SUB{\textgreater}1.6{\textless}/SUB{\textgreater} of nonrotating NSs with a mass of 1.6 \{M\}{\textless}SUB{\textgreater}? {\textless}/SUB{\textgreater} can be constrained to be larger than \{10.68\}{\textless}SUB{\textgreater}-0.04{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+0.15{\textless}/SUP{\textgreater} km, and the radius R {\textless}SUB{\textgreater}max{\textless}/SUB{\textgreater} of the nonrotating maximum-mass configuration must be larger than \{9.60\}{\textless}SUB{\textgreater}-0.03{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+0.14{\textless}/SUP{\textgreater} km. We point out that detections of future events will further improve these constraints. Moreover, we show that a future event with a signature of a prompt collapse of the merger remnant will establish even stronger constraints on the NS radius from above and the maximum mass M {\textless}SUB{\textgreater}max{\textless}/SUB{\textgreater} of NSs from above. These constraints are particularly robust because they only require a measurement of the chirp mass and a distinction between prompt and delayed collapse of the merger remnant, which may be inferred from the electromagnetic signal or even from the presence/absence of a ringdown gravitational-wave (GW) signal. This prospect strengthens the case of our novel method of constraining NS properties, which is directly applicable to future GW events with accompanying electromagnetic counterpart observations. We emphasize that this procedure is a new way of constraining NS radii from GW detections independent of existing efforts to infer radius information from the late inspiral phase or post-merger oscillations, and it does not require particularly loud GW events.},
	language = {en},
	number = {2},
	urldate = {2019-07-09},
	journal = {\apjl},
	author = {Bauswein, Andreas and Just, Oliver and Janka, Hans-Thomas and Stergioulas, Nikolaos},
	month = dec,
	year = {2017},
	pages = {L34},
	file = {Full Text PDF:/Users/dsiegel/Zotero/storage/GLPYZFV6/Bauswein et al. - 2017 - Neutron-star Radius Constraints from GW170817 and .pdf:application/pdf;Snapshot:/Users/dsiegel/Zotero/storage/ZI3JH6Z6/abstract.html:text/html}
}

